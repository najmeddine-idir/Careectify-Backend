{
  "name": "careectify-backend",
  "version": "1.0.0",
  "description": "The backend of the app Careectify",
  "exports": "./src/Api/bootstrap.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prelocal": "npm run swagger",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "start": "NODE_ENV=production node ./dist/bootstrap.js",
    "dev": "SET NODE_ENV=dev&& env-cmd -f ./Configurations/.env nodemon --exec node --loader ts-node/esm ./src/Api/bootstrap.ts",
    "local": "SET NODE_ENV=local&& env-cmd -f ./Configurations/.env nodemon --exec node --loader ts-node/esm ./src/Api/bootstrap.ts",
    "swagger": "tsoa spec"
  },
  "author": "Najmeddine Idir",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1471.0",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.3",
    "mongoose": "^7.6.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "ts-pattern": "^5.0.5",
    "tsoa": "^5.1.1",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.4",
    "@types/luxon": "^3.3.4",
    "@types/node": "^20.8.2",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.425.0",
    "winston": "^3.10.0"
  }
}
